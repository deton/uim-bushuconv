;;; ("候補" "annotation" (("画数" "代替候補文字列")))
(define bushuconv-bushu-annotation-alist
  '(("く" "(巛)")
    ("フ" "(幻)")
    ("L" "(直 断)")
    ("乀" "(刄 及)")
    ;; 2画
    ("ソ" "(並 并 挟 曽 柬 為)")
    ("ユ" "(侯)")
    ("ナ" "(右 左 布 友 有)")
    ("マ" "(通 勇)")
