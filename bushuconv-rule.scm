    ("み" "(巳)" "み (巳)")
    ("キ" "(奉 津)")
    ("ツ" "(畄 鼡 単)")
    ("囗" "くにがまえ (国 固)")
    ;; 4画
    ("ホ" "(余 朮 茶)")
    ("ヰ" "(舛)")
    ;; 5画
    ("電" "電-雨 (奄 竜)" "(電-雨)")
    ("売" "(壱 壷 壹 孛 / 殻 続)" (("5" "(売-儿)"))) ; 5画 / 7画
    ("並" "(虚 晋 普 霊 椪)" (("5" "(並の下側)"))) ; 5画 / 8画
    ;; 6画
    ("夾" "(峡 狭 / 俠 匧)" (("6" "(夹) {夾}"))) ; 6画 / 7画
